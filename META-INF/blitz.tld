<?xml version="1.0" encoding="UTF-8"?>
	<!--
		CTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library
		1.2//EN" </sp
	-->
<taglib>
	<tlibversion>1.2</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>tags-core</shortname>
	<uri>http://www.laxser.com/tags-core
	</uri>
	<tag>
		<name>flash</name>
		<tagclass>com.laxser.blitz.web.taglibs.FlashTag
		</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>key</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>prefix</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<name>suffix</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
	</tag>
</taglib>